<!doctype html>
<html>
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title> Senio Developers</title>

    

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>

    <link href='//fonts.googleapis.com/css?family=Lato:300,400' rel='stylesheet' type='text/css'>

    <%= stylesheet_link_tag :bootstrap %>
    <%= stylesheet_link_tag :style %>
    <%= javascript_include_tag :bootstrap %>


  </head>

  <body class="<%= page_classes %>">
    <header class="main-nav">
      <div class="wrapper">
       <nav class="navbar navbar-fixed-top " role="navigation" id='navtop'>
          <div class="container">
              <!-- Brand and toggle get grouped for better mobile display -->
              <div class="navbar-header">
                  <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                      <span class="sr-only">Toggle navigation</span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                      <span class="icon-bar"></span>
                  </button>
                  <a class="navbar-brand" href="/"><img class="media-object " src="logo.png" alt="..." height="40" id='senio-logo'
                style="    margin-left: 7px;
    margin-top: -16px;"></a>
              </div>
              <!-- Collect the nav links, forms, and other content for toggling -->
              <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                  <ul class="nav navbar-nav navbar-right">
                      <li >
                          <a href="/introduction" id='introduction'>Introduction</a>
                      </li>
                      <li>
                          <a href="#" id='developer_kit'>Developer Kit</a>
                      </li>
                      <li>
                          <a href="/examples" id='examples'>Examples</a>
                      </li>
                      <li>
                          <a href="/about" id='about'>About</a>
                      </li>
                      <li>
                          <a href="#" id='blog'>Blog</a>
                      </li>
                  </ul>
              </div>
              <!-- /.navbar-collapse -->
          </div>
          <!-- /.container -->
      </nav>
      </div>
    </header>

    

    <%= yield %>

    <footer class="footer">
      <div class="container" style="text-align:center">
        
         
        <a href="#" class="text-muted">Senio Home</a> |
        <a href="#" class="text-muted">Careers</a> |
        <a href="#" class="text-muted">Contact</a> 
        <p>&copy; Pentalist
      
        
      </div>
    </footer>
    <%= javascript_include_tag :all %>
  </body>
</html>
